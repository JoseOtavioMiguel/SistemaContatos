<div class="text-center">
    <h1 class="display-4">Criar Contato</h1>
</div>

<form>
    <div class="mb-3">
        <label for="nome" class="form-label">Nome</label>
        <input type="text" class="form-control"  placeholder="Digite o nome do contato">
    </div>
    <div class="mb-3">
        <label for="email" class="form-label">E-mail</label>
        <input type="text" class="form-control" placeholder="Digite o email do contato">
    </div>
    <div class="mb-3">
        <label for="celular" class="form-label">Celular</label>
        <input type="text" class="form-control" placeholder="Digite o celular do contato">
    </div>
    <button type="submit" class="btn btn-primary btn-block">Adicionar</button>
    <a class="btn btn-secondary d-grid gap-2 mt-2" asp-controller="Contato" asp-action="Index" role="button">Voltar</a>
</form>
