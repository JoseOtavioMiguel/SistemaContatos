<div class="text-center">
    <h1 class="display-4">Editar Contato</h1>
</div>

<form>
    <div class="form-group mb-3">
        <label for="nome">Nome</label>
        <input type="text" class="form-control" placeholder="Digite o nome do contato">
    </div>
    <div class="form-group mb-3">
        <label for="email">E-mail</label>
        <input type="text" class="form-control" placeholder="Digite o email do contato">
    </div>
    <div class="form-group mb-3">
        <label for="celular">Celular</label>
        <input type="text" class="form-control" placeholder="Digite o celular do contato">
    </div>
    <button type="submit" class="btn btn-primary">Adicionar</button>
    <a class="btn btn-secondary " asp-controller="Contato" asp-action="Index" role="button">Voltar</a>
</form>
